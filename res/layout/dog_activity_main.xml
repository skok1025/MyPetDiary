	
	
	
	
	
	
	
	<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/introback"
    android:gravity="center_vertical"
    tools:context=".DogMainActivity" >

    <Button
        android:id="@+id/button3"
        android:layout_width="90dp"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/button1"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/button2"
        android:layout_marginLeft="48dp"
        android:background="@drawable/but3"
        android:onClick="btnClick3" />

    <Button
        android:id="@+id/button5"
        android:layout_width="90dp"
        android:layout_height="wrap_content"
        android:layout_above="@+id/button6"
        android:layout_alignLeft="@+id/button2"
        android:layout_alignTop="@+id/button4"
        android:background="@drawable/but5"
        android:onClick="btnClick5" />

    <Button
        android:id="@+id/button2"
        android:layout_width="90dp"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/button6"
        android:layout_alignLeft="@+id/button3"
        android:background="@drawable/but2"
        android:onClick="btnClick2" />

    <Button
        android:id="@+id/button4"
        android:layout_width="90dp"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:layout_marginRight="38dp"
        android:layout_marginTop="110dp"
        android:background="@drawable/but4"
        android:onClick="btnClick4" />

    <Button
        android:id="@+id/button6"
        android:layout_width="90dp"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/button4"
        android:layout_marginRight="38dp"
        android:background="@drawable/but6"
        android:onClick="btnClick6" />

    <Button
        android:id="@+id/button1"
        android:layout_width="90dp"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/button6"
        android:layout_marginRight="38dp"
        android:background="@drawable/but1"
        android:onClick="btnClick1" />

</RelativeLayout>